<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="?android:attr/colorControlHighlight">
    <item>
        <selector>
            <item android:state_enabled="false">
                <shape>
                    <solid android:color="?attr/tcx_fillQuarternaryBackground"/>
                    <corners android:radius="@dimen/space"/>
                </shape>
            </item>
            <item>
                <shape>
                    <solid android:color="?attr/tcx_textPrimary"/>
                    <corners android:radius="@dimen/space"/>
                </shape>
            </item>
        </selector>
    </item>
    <item android:id="@android:id/mask">
        <shape>
            <stroke
                android:width="@dimen/biz_acs_start_call_survey_button_border_width"
                android:color="?attr/tcx_textTertiary"/>
            <solid android:color="@android:color/white"/>
            <corners android:radius="@dimen/space"/>
        </shape>
    </item>
</ripple>
