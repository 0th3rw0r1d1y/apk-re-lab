<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <ripple android:color="?attr/tcx_buttonRippleColor">
            <item android:id="@android:id/mask">
                <shape android:shape="rectangle">
                    <corners android:radius="@dimen/background_tcx_outline_radius"/>
                    <solid android:color="#ffffff"/>
                </shape>
            </item>
        </ripple>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/background_tcx_outline_radius"/>
            <stroke
                android:width="1dp"
                android:color="?attr/tcx_fillTertiaryBackground"/>
        </shape>
    </item>
</layer-list>
