<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="?attr/tcx_buttonRippleColor">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="#000000"/>
            <corners android:radius="42dp"/>
        </shape>
    </item>
    <item android:id="@+id/outline">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="?attr/tcx_goldDivider"/>
            <corners android:radius="42dp"/>
        </shape>
    </item>
</ripple>
