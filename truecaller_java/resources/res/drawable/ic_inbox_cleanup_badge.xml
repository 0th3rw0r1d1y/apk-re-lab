<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/ic_inbox_cleanup"/>
    <item
        android:gravity="top|right"
        android:top="-2dp"
        android:right="-2dp">
        <shape android:shape="oval">
            <size
                android:height="12dp"
                android:width="12dp"/>
            <solid android:color="?attr/tcx_brandBackgroundBlue"/>
            <stroke
                android:width="1dp"
                android:color="?attr/tcx_backgroundTertiary"/>
        </shape>
    </item>
</layer-list>
