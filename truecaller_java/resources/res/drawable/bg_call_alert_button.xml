<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="?android:attr/colorControlHighlight">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="@color/tcx_brandBackgroundBlue_light"/>
            <corners android:radius="8dp"/>
        </shape>
    </item>
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <corners android:radius="8dp"/>
            <solid android:color="@color/tcx_brandBackgroundBlue_light"/>
        </shape>
    </item>
</ripple>
