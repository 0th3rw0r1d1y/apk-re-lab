<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/card_dark"/>
        </shape>
    </item>
    <item
        android:gravity="center"
        android:left="1dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="1dp">
        <shape android:shape="oval">
            <solid android:color="@color/accent_dark"/>
        </shape>
    </item>
    <item
        android:gravity="center"
        android:drawable="@drawable/ic_clear_white_14dp"
        android:left="3dp"
        android:top="3dp"
        android:right="3dp"
        android:bottom="3dp"/>
</layer-list>
