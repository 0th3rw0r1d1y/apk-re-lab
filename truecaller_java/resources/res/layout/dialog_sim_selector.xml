<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/rounded_corners_rectangle"
    android:paddingBottom="20dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:minWidth="312dp"
    android:backgroundTint="?attr/tc_color_containerFillCustom">
    <androidx.appcompat.widget.AppCompatTextView
        android:textSize="18sp"
        android:textColor="@color/dialog_item_title_selector"
        android:ellipsize="end"
        android:id="@+id/title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="@string/incallui_phone_accounts_dialog_title"
        android:maxLines="1"
        android:lineSpacingExtra="3sp"
        android:fontFamily="@font/roboto_medium"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <View
        android:id="@+id/sim1Container"
        android:background="?attr/selectableItemBackground"
        android:focusable="true"
        android:clickable="true"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/sim1Subtitle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/sim1Title"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/sim1Img"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/sim1Subtitle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/sim1Title"
        app:srcCompat="@drawable/ic_tcx_action_call_sim_1_24dp"/>
    <androidx.appcompat.widget.AppCompatTextView
        android:textSize="14sp"
        android:textColor="@color/dialog_item_title_selector"
        android:id="@+id/sim1Title"
        android:paddingTop="12dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:lineSpacingExtra="2sp"
        android:fontFamily="@font/roboto_medium"
        android:layout_marginStart="56dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title"/>
    <androidx.appcompat.widget.AppCompatTextView
        android:textSize="14sp"
        android:textColor="@color/dialog_item_subtitle_selector"
        android:ellipsize="end"
        android:id="@+id/sim1Phone"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:lineSpacingExtra="2sp"
        android:fontFamily="@font/roboto_medium"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintBottom_toBottomOf="@+id/sim1Title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/sim1Title"/>
    <androidx.appcompat.widget.AppCompatTextView
        android:textSize="14sp"
        android:textColor="@color/dialog_item_subtitle_selector"
        android:ellipsize="end"
        android:id="@+id/sim1Subtitle"
        android:paddingBottom="12dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:maxLines="1"
        android:lineSpacingExtra="2sp"
        android:fontFamily="@font/roboto_regular"
        android:layout_marginEnd="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/sim1Title"
        app:layout_constraintTop_toBottomOf="@+id/sim1Title"/>
    <View
        android:id="@+id/sim2Container"
        android:background="?attr/selectableItemBackground"
        android:focusable="true"
        android:clickable="true"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/sim2Subtitle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/sim2Title"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/sim2Img"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/sim2Subtitle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/sim2Title"
        app:srcCompat="@drawable/ic_tcx_action_call_sim_2_24dp"/>
    <androidx.appcompat.widget.AppCompatTextView
        android:textSize="14sp"
        android:textColor="@color/dialog_item_title_selector"
        android:id="@+id/sim2Title"
        android:paddingTop="12dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:lineSpacingExtra="2sp"
        android:fontFamily="@font/roboto_medium"
        android:layout_marginStart="56dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sim1Subtitle"/>
    <androidx.appcompat.widget.AppCompatTextView
        android:textSize="14sp"
        android:textColor="@color/dialog_item_subtitle_selector"
        android:ellipsize="end"
        android:id="@+id/sim2Phone"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:maxLines="1"
        android:lineSpacingExtra="2sp"
        android:fontFamily="@font/roboto_medium"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintBottom_toBottomOf="@+id/sim2Title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/sim2Title"/>
    <androidx.appcompat.widget.AppCompatTextView
        android:textSize="14sp"
        android:textColor="@color/dialog_item_subtitle_selector"
        android:ellipsize="end"
        android:id="@+id/sim2Subtitle"
        android:paddingBottom="12dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:maxLines="1"
        android:lineSpacingExtra="2sp"
        android:fontFamily="@font/roboto_regular"
        android:layout_marginStart="56dp"
        android:layout_marginEnd="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sim2Title"/>
    <androidx.appcompat.widget.AppCompatCheckBox
        android:id="@+id/defaultSimCheckbox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:buttonTint="@color/selector_checkbox_tint_color"
        app:layout_constraintBottom_toBottomOf="@+id/defaultSimText"
        app:layout_constraintEnd_toStartOf="@+id/defaultSimText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/defaultSimText"/>
    <androidx.appcompat.widget.AppCompatTextView
        android:textColor="@color/dialog_item_subtitle_selector"
        android:id="@+id/defaultSimText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/pref_set_default_sim"
        android:lineSpacingExtra="2sp"
        android:fontFamily="@font/roboto_regular"
        android:layout_marginStart="56dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sim2Subtitle"/>
</androidx.constraintlayout.widget.ConstraintLayout>
