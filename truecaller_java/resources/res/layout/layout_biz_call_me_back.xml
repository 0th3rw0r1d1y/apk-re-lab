<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cardViewCallMeBack"
    android:background="@color/quantum_black_100"
    android:visibility="gone"
    android:clickable="true"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/space"
    app:cardBackgroundColor="@color/quantum_black_100"
    app:cardCornerRadius="@dimen/space"
    app:cardElevation="@dimen/semiSpace"
    app:cardPreventCornerOverlap="false"
    app:cardUseCompatPadding="false">
    <com.truecaller.bizmon.callMeBack.mvp.BizCallMeBackWithSlotsView
        android:id="@+id/callMeBackWithSlotsView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</androidx.cardview.widget.CardView>
