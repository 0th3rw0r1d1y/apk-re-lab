<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/wsfm_no_search_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/WhoSearchedForMeNoNewSearchAppearanceTxt"
        app:layout_constraintBottom_toTopOf="@+id/wsfm_no_search_appearance"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"
        app:srcCompat="@drawable/ic_wsfm_empty"/>
    <TextView
        android:id="@+id/wsfm_no_search_appearance"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/WhoSearchedForMeNoNewSearchAppearanceTxt"
        android:textAlignment="center"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toTopOf="@+id/wsfm_no_search_appearance_desc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wsfm_no_search_image"
        style="@style/StyleX.Text.Headline.H6"/>
    <TextView
        android:textColor="?attr/tcx_textTertiary"
        android:id="@+id/wsfm_no_search_appearance_desc"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/WhoSearchedForMeNoSearchAppearanceDesc"
        android:textAlignment="center"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wsfm_no_search_appearance"
        style="@style/StyleX.Text.Roboto.Regular.16"/>
</androidx.constraintlayout.widget.ConstraintLayout>
