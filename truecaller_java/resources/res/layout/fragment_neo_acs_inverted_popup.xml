<?xml version="1.0" encoding="utf-8"?>
<com.truecaller.acs.ui.view.CoordinatorLayoutWithTouchInterceptor xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/root"
    android:focusable="true"
    android:clickable="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_gravity="center_vertical"
        android:orientation="vertical"
        android:id="@+id/acsContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="8dp"
        android:paddingEnd="8dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_gravity="center_vertical"
            android:id="@+id/acsAdsContainerFrameLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/mainContainer">
            <include
                android:id="@+id/acsAdsContainerLayout"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/control_space"
                app:layout_constrainedHeight="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_max="@dimen/popup_acs_ads_placeholder_height"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                layout="@layout/layout_neo_acs_ads_container"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/partner_container"
            android:paddingBottom="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/acsAdsContainerFrameLayout">
            <ImageView
                android:id="@+id/truecallerLogo"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:adjustViewBounds="true"
                android:contentDescription="@null"
                android:layoutDirection="ltr"/>
            <View
                android:layout_gravity="center_vertical"
                android:id="@+id/logoDivider"
                android:background="@color/popup_acs_logo_divider"
                android:layout_width="1dp"
                android:layout_height="12dp"
                android:layout_marginStart="6dp"
                android:layout_marginEnd="6dp"/>
            <ImageView
                android:layout_gravity="bottom"
                android:id="@+id/partnerLogo"
                android:layout_width="wrap_content"
                android:layout_height="12dp"
                android:adjustViewBounds="true"
                android:contentDescription="@null"
                android:layoutDirection="ltr"/>
            <ImageView
                android:layout_gravity="center_vertical"
                android:id="@+id/truecallerLogoQuestionMark"
                android:visibility="gone"
                android:layout_width="14dp"
                android:layout_height="14dp"
                android:adjustViewBounds="true"
                android:contentDescription="@null"
                android:layout_marginStart="2dp"
                app:srcCompat="@drawable/ic_question_mark"/>
        </LinearLayout>
        <com.truecaller.acs.ui.AvatarTabIndicator
            android:gravity="end"
            android:orientation="horizontal"
            android:id="@+id/acsPicker"
            android:paddingBottom="4dp"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/acsAdsContainerFrameLayout"
            android:layout_alignParentRight="true"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/mainContainer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/bottomLayout"
            android:animateLayoutChanges="true">
            <View
                android:id="@+id/headerBackground"
                android:background="@drawable/background_popup_header"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/detailsBackground"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <com.truecaller.videocallerid.ui.videoplayer.AvatarVideoPlayerView
                android:id="@+id/videoAvatarXView"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="8dp"
                android:contentDescription="@null"
                android:layout_marginStart="12dp"
                android:elevation="2dp"
                app:invertVerifiedRingColor="true"
                app:layout_constraintBottom_toTopOf="@+id/avatarDisclaimerBarrier"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0"/>
            <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:id="@+id/avatarGuideline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:elevation="1dp"
                app:layout_constraintGuide_begin="84dp"/>
            <ImageView
                android:id="@+id/closeIcon"
                android:background="?attr/selectableItemBackground"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:scaleType="center"
                android:minWidth="24dp"
                android:minHeight="24dp"
                android:contentDescription="@null"
                android:elevation="1dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_close"
                app:tint="@color/tcx_textTertiary_dark"/>
            <ImageView
                android:id="@+id/simIndicator"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:contentDescription="@null"
                android:layout_marginEnd="2dp"
                android:elevation="1dp"
                app:layout_constraintBottom_toBottomOf="@+id/callTypeAndTime"
                app:layout_constraintEnd_toStartOf="@+id/callTypeAndTime"
                app:layout_constraintStart_toEndOf="@+id/avatarGuideline"
                app:layout_constraintTop_toTopOf="@+id/callTypeAndTime"
                app:srcCompat="@drawable/ic_sim_1_large_font_16dp"
                app:tint="@color/tc_color_containerFillPrimary_light"/>
            <TextView
                android:textColor="@color/popup_acs_header_main_text_default"
                android:ellipsize="end"
                android:id="@+id/callTypeAndTime"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:maxLines="1"
                android:elevation="1dp"
                app:emojiCompatEnabled="false"
                app:layout_constraintBottom_toTopOf="@+id/nameOrNumber"
                app:layout_constraintEnd_toStartOf="@+id/closeIcon"
                app:layout_constraintStart_toEndOf="@+id/simIndicator"
                app:layout_constraintTop_toTopOf="parent"
                style="@style/StyleX.Text.Caption"/>
            <TextView
                android:textColor="@color/popup_acs_header_main_text_default"
                android:ellipsize="marquee"
                android:id="@+id/nameOrNumber"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:scrollHorizontally="true"
                android:singleLine="true"
                android:includeFontPadding="false"
                android:marqueeRepeatLimit="marquee_forever"
                android:elevation="1dp"
                app:emojiCompatEnabled="false"
                app:layout_constraintBottom_toTopOf="@+id/reportSpamInfo"
                app:layout_constraintEnd_toStartOf="@+id/verified"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="@+id/avatarGuideline"
                app:layout_constraintTop_toBottomOf="@+id/callTypeAndTime"
                app:layout_constraintWidth_default="wrap"
                style="@style/StyleX.Text.Headline.H6"/>
            <ImageView
                android:id="@+id/verified"
                android:visibility="gone"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginTop="2dp"
                android:contentDescription="@null"
                android:layout_marginStart="4dp"
                android:elevation="1dp"
                app:layout_constraintBottom_toBottomOf="@+id/nameOrNumber"
                app:layout_constraintEnd_toStartOf="@+id/closeIcon"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toEndOf="@+id/nameOrNumber"
                app:layout_constraintTop_toTopOf="@+id/nameOrNumber"
                app:srcCompat="@drawable/ic_tcx_verified_badge"
                app:tint="@color/popup_acs_icon_tint_white"/>
            <LinearLayout
                android:layout_gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/changeName"
                android:background="@drawable/bg_acs_change_view"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="16dp"
                android:elevation="1dp"
                app:layout_constraintBottom_toBottomOf="@+id/nameOrNumber"
                app:layout_constraintEnd_toStartOf="@+id/closeIcon"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toEndOf="@+id/verified"
                app:layout_constraintTop_toTopOf="@+id/nameOrNumber"
                app:layout_goneMarginStart="4dp">
                <ImageView
                    android:id="@+id/changeNameIcon"
                    android:background="@drawable/bg_acs_change_icon"
                    android:padding="2dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/ic_edit_12dp"/>
                <TextView
                    android:id="@+id/changeNameText"
                    android:paddingTop="1dp"
                    android:layout_width="wrap_content"
                    android:layout_height="16dp"
                    android:text="@string/global_change"
                    android:textAllCaps="true"
                    android:paddingStart="2dp"
                    android:paddingEnd="4dp"
                    app:emojiCompatEnabled="false"
                    style="@style/StyleX.Text.Caption.C1"/>
            </LinearLayout>
            <TextView
                android:textColor="@color/popup_acs_header_main_text_default"
                android:ellipsize="end"
                android:id="@+id/reportSpamInfo"
                android:visibility="gone"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:text="@string/acs_caller_label_reported_spam"
                android:maxLines="1"
                android:includeFontPadding="false"
                android:layout_marginEnd="12dp"
                android:elevation="1dp"
                app:emojiCompatEnabled="false"
                app:layout_constraintBottom_toTopOf="@+id/bizTagText"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/avatarGuideline"
                app:layout_constraintTop_toBottomOf="@+id/nameOrNumber"
                style="@style/StyleX.Text.Subtitle.S2"/>
            <TextView
                android:id="@+id/bizTagText"
                android:visibility="gone"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/control_mini_space"
                android:layout_marginBottom="@dimen/control_space"
                android:layout_marginEnd="@dimen/control_space"
                app:emojiCompatEnabled="false"
                app:layout_constraintBottom_toTopOf="@+id/contactDetails"
                app:layout_constraintEnd_toStartOf="@+id/bizBadgeText"
                app:layout_constraintStart_toStartOf="@+id/avatarGuideline"
                app:layout_constraintTop_toBottomOf="@+id/reportSpamInfo"
                style="@style/BizTagTextStyle"/>
            <TextView
                android:id="@+id/bizBadgeText"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/extraSpace"
                app:emojiCompatEnabled="false"
                app:layout_constraintBottom_toBottomOf="@+id/bizTagText"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/bizTagText"
                style="@style/BizBadgeTextStyle"/>
            <TextView
                android:textColor="@color/popup_acs_header_main_text_default"
                android:ellipsize="end"
                android:id="@+id/contactDetails"
                android:visibility="gone"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:maxLines="1"
                android:includeFontPadding="false"
                android:layout_marginEnd="12dp"
                android:elevation="1dp"
                app:emojiCompatEnabled="false"
                app:layout_constraintBottom_toTopOf="@+id/businessStatus"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/avatarGuideline"
                app:layout_constraintTop_toBottomOf="@+id/bizTagText"
                style="@style/StyleX.Text.Subtitle.S2"/>
            <TextView
                android:textColor="@color/popup_acs_header_main_text_default"
                android:ellipsize="end"
                android:id="@+id/businessStatus"
                android:visibility="gone"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:maxLines="1"
                android:layout_marginEnd="12dp"
                android:elevation="1dp"
                app:emojiCompatEnabled="false"
                app:layout_constraintBottom_toTopOf="@+id/priority"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/avatarGuideline"
                app:layout_constraintTop_toBottomOf="@+id/contactDetails"
                style="@style/StyleX.Text.Caption.Medium"/>
            <ImageView
                android:id="@+id/priorityIcon"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:contentDescription="@string/acs_caller_label_priority_call"
                android:elevation="1dp"
                app:layout_constraintBottom_toBottomOf="@+id/priority"
                app:layout_constraintStart_toStartOf="@+id/avatarGuideline"
                app:layout_constraintTop_toTopOf="@+id/priority"
                app:srcCompat="@drawable/ic_priority"/>
            <TextView
                android:textColor="@color/popup_acs_header_main_text_default"
                android:ellipsize="end"
                android:id="@+id/priority"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text="@string/acs_caller_label_priority_call"
                android:maxLines="1"
                android:textAllCaps="true"
                android:layout_marginStart="2dp"
                android:layout_marginEnd="12dp"
                android:elevation="1dp"
                app:emojiCompatEnabled="false"
                app:layout_constraintBottom_toTopOf="@+id/tagViewBusiness"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/priorityIcon"
                app:layout_constraintTop_toBottomOf="@+id/businessStatus"
                style="@style/StyleX.Text.Subtitle.S2"/>
            <androidx.constraintlayout.widget.Group
                android:id="@+id/priorityGroup"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:elevation="1dp"
                app:constraint_referenced_ids="priority,priorityIcon"/>
            <com.truecaller.common.ui.tag.TagXView
                android:id="@+id/tagViewBusiness"
                android:focusable="false"
                android:visibility="gone"
                android:clickable="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:elevation="1dp"
                app:layout_constraintBottom_toTopOf="@+id/secureCallView"
                app:layout_constraintStart_toStartOf="@+id/avatarGuideline"
                app:layout_constraintTop_toBottomOf="@+id/priority"/>
            <TextView
                android:textColor="@color/white"
                android:gravity="center_vertical"
                android:id="@+id/secureCallView"
                android:background="@drawable/background_tcx_tag"
                android:paddingTop="6dp"
                android:paddingBottom="6dp"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text="@string/acs_caller_secure_call_badge"
                android:drawablePadding="4dp"
                android:paddingStart="12dp"
                android:paddingEnd="16dp"
                android:backgroundTint="#1affffff"
                app:drawableStartCompat="@drawable/ic_secure_call"
                app:layout_constraintBottom_toTopOf="@+id/infoLineSpam"
                app:layout_constraintStart_toStartOf="@+id/avatarGuideline"
                app:layout_constraintTop_toBottomOf="@+id/tagViewBusiness"
                style="@style/StyleX.Text.Subtitle.S2"/>
            <include
                android:id="@+id/infoLineSpam"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="16dp"
                app:layout_constrainedWidth="true"
                app:layout_constraintBottom_toTopOf="@+id/tagView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintStart_toStartOf="@+id/avatarGuideline"
                app:layout_constraintTop_toBottomOf="@+id/secureCallView"
                layout="@layout/include_spam_data_container"/>
            <com.truecaller.common.ui.tag.TagXView
                android:id="@+id/tagView"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:elevation="1dp"
                app:layout_constraintBottom_toTopOf="@+id/truecontext"
                app:layout_constraintStart_toStartOf="@+id/avatarGuideline"
                app:layout_constraintTop_toBottomOf="@+id/infoLineSpam"/>
            <com.truecaller.truecontext.TrueContext
                android:id="@+id/truecontext"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="16dp"
                android:elevation="1dp"
                app:layout_constraintBottom_toTopOf="@+id/bizCallReasonRevampedViewStub"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/avatarGuideline"
                app:layout_constraintTop_toBottomOf="@+id/tagView"
                app:view_orientation="start"
                app:view_size="small"/>
            <ViewStub
                android:id="@+id/bizCallReasonRevampedViewStub"
                android:visibility="gone"
                android:layout="@layout/view_stub_biz_call_reason_revamped"
                android:inflatedId="@+id/bizCallReasonRevampedViewStub"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/space"
                android:layout_marginStart="@dimen/space"
                android:layout_marginEnd="@dimen/space"
                app:layout_constraintBottom_toTopOf="@+id/disclaimerText"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/truecontext"/>
            <ViewStub
                android:id="@+id/manualCallerIdView"
                android:layout="@layout/include_manual_caller_id_acs_view"
                android:inflatedId="@+id/manualCallerIdView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toTopOf="@+id/disclaimerText"
                app:layout_constraintEnd_toStartOf="@+id/closeIcon"
                app:layout_constraintStart_toStartOf="@+id/avatarGuideline"
                app:layout_constraintTop_toBottomOf="@+id/callTypeAndTime"/>
            <ViewStub
                android:id="@+id/softThrottledCallerIdView"
                android:layout="@layout/include_soft_throttled_caller_id_acs_view"
                android:inflatedId="@+id/softThrottledCallerIdView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toTopOf="@+id/disclaimerText"
                app:layout_constraintEnd_toStartOf="@+id/closeIcon"
                app:layout_constraintStart_toStartOf="@+id/avatarGuideline"
                app:layout_constraintTop_toBottomOf="@+id/callTypeAndTime"/>
            <androidx.constraintlayout.widget.Barrier
                android:id="@+id/mainContentBarrier"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:barrierDirection="bottom"
                app:constraint_referenced_ids="truecontext, manualCallerIdView, softThrottledCallerIdView,bizCallReasonRevampedViewStub"/>
            <TextView
                android:textSize="10sp"
                android:textColor="@color/popup_acs_header_main_text_default"
                android:id="@+id/disclaimerText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="8dp"
                android:text="@string/acs_disclaimer_text"
                android:drawablePadding="3dp"
                app:drawableStartCompat="@drawable/ic_acs_disclaimer_icon"
                app:emojiCompatEnabled="false"
                app:layout_constraintBottom_toTopOf="@+id/avatarDisclaimerBarrier"
                app:layout_constraintStart_toStartOf="@+id/avatarGuideline"
                app:layout_constraintTop_toBottomOf="@+id/mainContentBarrier"/>
            <androidx.constraintlayout.widget.Barrier
                android:id="@+id/avatarDisclaimerBarrier"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:elevation="1dp"
                app:barrierDirection="bottom"
                app:constraint_referenced_ids="videoAvatarXView,disclaimerText"/>
            <com.google.android.material.button.MaterialButton
                android:id="@+id/viewProfile"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:text="@string/acs_view_profile"
                android:insetTop="0dp"
                android:insetBottom="0dp"
                android:textAllCaps="false"
                android:elevation="1dp"
                app:backgroundTint="@color/acs_popup_view_profile_default"
                app:elevation="4dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/avatarDisclaimerBarrier"
                app:rippleColor="?android:attr/colorControlHighlight"
                style="@style/StyleX.Button.Small"/>
            <View
                android:id="@+id/headerDividerLine"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_marginTop="8dp"
                android:elevation="1dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/viewProfile"/>
            <FrameLayout
                android:id="@+id/placeholder_acs_reply"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:elevation="4dp"
                app:layout_constraintBottom_toTopOf="@+id/action_button_barrier"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/headerDividerLine"/>
            <View
                android:id="@+id/detailsBackground"
                android:background="?attr/tc_color_containerFillCustom"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toTopOf="@+id/action_container"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/headerDividerLine"/>
            <TextView
                android:ellipsize="end"
                android:id="@+id/phoneNumber"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:maxLines="1"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                app:emojiCompatEnabled="false"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/headerDividerLine"
                style="@style/StyleX.Text.Body.B2"/>
            <com.truecaller.timezone.AddressTimezoneView
                android:textSize="14sp"
                android:id="@+id/addressTimezoneView"
                android:visibility="gone"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                app:addressTextColor="?attr/tcx_textSecondary"
                app:layout_constrainedWidth="true"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/phoneNumber"
                app:singleLine="true"
                app:timezoneIconColor="?attr/colorAccent"
                app:timezoneTextColor="?attr/colorAccent"/>
            <com.truecaller.acspromo.uicomponents.AcsPromoView
                android:id="@+id/spamPremiumPromoView"
                android:visibility="gone"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toTopOf="@+id/action_button_barrier"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/addressTimezoneView"/>
            <androidx.fragment.app.FragmentContainerView
                android:id="@+id/survey_container"
                android:visibility="gone"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:elevation="2dp"
                app:layout_constraintBottom_toTopOf="@+id/action_button_barrier"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/headerDividerLine"/>
            <androidx.compose.ui.platform.ComposeView
