<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/settings_nav_graph"
    app:startDestination="@+id/categories">
    <fragment
        android:name="com.truecaller.settings.impl.ui.categories.CategoriesFragment"
        android:id="@+id/categories">
        <action
            android:id="@+id/to_search"
            app:destination="@+id/search"/>
        <action
            android:id="@+id/to_calls"
            app:destination="@+id/calls"/>
        <action
            android:id="@+id/to_block"
            app:destination="@+id/block"/>
        <action
            android:id="@+id/to_family_protect"
            app:destination="@+id/family_protect"/>
        <action
            android:id="@+id/to_about"
            app:destination="@+id/about"/>
        <action
            android:id="@+id/to_general"
            app:destination="@+id/general"/>
        <action
            android:id="@+id/to_call_assistant"
            app:destination="@+id/call_assistant"/>
        <action
            android:id="@+id/to_call_assistant_inclusive"
            app:destination="@+id/call_assistant"
            app:popUpTo="@+id/categories"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/to_privacy"
            app:destination="@+id/privacy"/>
        <action
            android:id="@+id/to_premium"
            app:destination="@+id/premium"/>
        <action
            android:id="@+id/to_watch"
            app:destination="@+id/watch"/>
        <action
            android:id="@+id/to_messaging"
            app:destination="@+id/messaging"/>
        <action
            android:id="@+id/to_help"
            app:destination="@+id/help"/>
        <action
            android:id="@+id/to_voicemail"
            app:destination="@+id/voicemail"/>
    </fragment>
    <fragment
        android:name="com.truecaller.settings.impl.ui.search.SearchSettingsFragment"
        android:id="@+id/search">
        <action
            android:id="@+id/to_calls"
            app:destination="@+id/calls"/>
        <action
            android:id="@+id/to_block"
            app:destination="@+id/block"/>
        <action
            android:id="@+id/to_about"
            app:destination="@+id/about"/>
        <action
            android:id="@+id/to_general"
            app:destination="@+id/general"/>
        <action
            android:id="@+id/to_call_assistant"
            app:destination="@+id/call_assistant"/>
        <action
            android:id="@+id/to_privacy"
            app:destination="@+id/privacy"/>
        <action
            android:id="@+id/to_premium"
            app:destination="@+id/premium"/>
        <action
            android:id="@+id/to_watch"
            app:destination="@+id/watch"/>
        <action
            android:id="@+id/to_messaging"
            app:destination="@+id/messaging"/>
        <action
            android:id="@+id/to_help"
            app:destination="@+id/help"/>
    </fragment>
    <fragment
        android:name="com.truecaller.settings.impl.ui.calls.CallsSettingsFragment"
        android:id="@+id/calls">
        <argument
            android:name="analytics_context"
            android:defaultValue="settings_screen"
            app:argType="string"/>
        <argument
            android:name="settingItem"
            android:defaultValue="@null"
            app:argType="com.truecaller.settings.impl.ui.calls.CallsSettings"
            app:nullable="true"/>
        <argument
            android:name="autoEnableHideAcsNonPb"
            android:defaultValue="false"
            app:argType="boolean"/>
        <argument
            android:name="autoRetryMissedCallSwitchChangeRequest"
            android:defaultValue="false"
            app:argType="boolean"/>
    </fragment>
    <fragment
        android:name="com.truecaller.settings.impl.ui.block.BlockSettingsFragment"
        android:id="@+id/block">
        <argument
            android:name="source"
            android:defaultValue="UNKNOWN"
            app:argType="com.truecaller.settings.api.SettingsSource"/>
        <argument
            android:name="analytics_context"
            android:defaultValue="settings_screen"
            app:argType="string"/>
        <argument
            android:name="settingItem"
            android:defaultValue="@null"
            app:argType="com.truecaller.settings.impl.ui.block.BlockSettings"
            app:nullable="true"/>
        <argument
            android:name="updateSpamList"
            android:defaultValue="false"
            app:argType="boolean"/>
    </fragment>
    <fragment
        android:name="com.truecaller.settings.impl.ui.about.AboutSettingsFragment"
        android:id="@+id/about">
        <argument
            android:name="analytics_context"
            android:defaultValue="settings_screen"
            app:argType="string"/>
        <argument
            android:name="settingItem"
            android:defaultValue="@null"
            app:argType="com.truecaller.settings.impl.ui.about.AboutSettings"
            app:nullable="true"/>
    </fragment>
    <fragment
        android:name="com.truecaller.settings.impl.ui.familyprotect.FamilyProtectSettingsFragment"
        android:id="@+id/family_protect">
        <argument
            android:name="analytics_context"
            android:defaultValue="settings_screen"
            app:argType="string"/>
    </fragment>
    <fragment
        android:name="com.truecaller.settings.impl.ui.voicemail.VoicemailSettingsFragment"
        android:id="@+id/voicemail">
        <argument
            android:name="analytics_context"
            android:defaultValue="settings_screen"
            app:argType="string"/>
    </fragment>
    <fragment
        android:name="com.truecaller.settings.impl.ui.general.GeneralSettingsFragment"
        android:id="@+id/general">
        <argument
            android:name="analytics_context"
            android:defaultValue="settings_screen"
            app:argType="string"/>
        <argument
            android:name="settingItem"
            android:defaultValue="@null"
            app:argType="com.truecaller.settings.impl.ui.general.GeneralSettings"
            app:nullable="true"/>
    </fragment>
    <fragment
        android:name="com.truecaller.settings.impl.ui.call_assistant.CallAssistantSettingsFragment"
        android:id="@+id/call_assistant">
        <argument
            android:name="analytics_context"
            android:defaultValue="settings_screen"
            app:argType="string"/>
        <argument
            android:name="settingItem"
            android:defaultValue="@null"
            app:argType="com.truecaller.settings.impl.ui.call_assistant.CallAssistantSettings"
            app:nullable="true"/>
        <argument
            android:name="navigateToItem"
            android:defaultValue="@null"
            app:argType="com.truecaller.settings.impl.ui.call_assistant.CallAssistantSettings"
            app:nullable="true"/>
        <argument
            android:name="finishOnBackPress"
            android:defaultValue="false"
            app:argType="boolean"/>
        <action
            android:id="@+id/assistantToBlock"
            app:destination="@+id/block"/>
    </fragment>
    <fragment
        android:name="com.truecaller.settings.impl.ui.privacy.PrivacySettingsFragment"
        android:id="@+id/privacy">
        <argument
            android:name="analytics_context"
            android:defaultValue="settings_screen"
            app:argType="string"/>
        <argument
            android:name="settingItem"
            android:defaultValue="@null"
            app:argType="com.truecaller.settings.impl.ui.privacy.PrivacySettings"
            app:nullable="true"/>
    </fragment>
    <fragment
        android:name="com.truecaller.settings.impl.ui.premium.PremiumSettingsFragment"
        android:id="@+id/premium">
        <argument
            android:name="analytics_context"
            android:defaultValue="settings_screen"
            app:argType="string"/>
        <argument
            android:name="settingItem"
            android:defaultValue="@null"
            app:argType="com.truecaller.settings.impl.ui.premium.PremiumSettings"
            app:nullable="true"/>
    </fragment>
    <fragment
        android:name="com.truecaller.settings.impl.ui.watch.WatchSettingsFragment"
        android:id="@+id/watch">
        <argument
            android:name="analytics_context"
            android:defaultValue="settings_screen"
            app:argType="string"/>
        <argument
            android:name="settingItem"
            android:defaultValue="@null"
            app:argType="com.truecaller.settings.impl.ui.watch.WatchSettings"
            app:nullable="true"/>
    </fragment>
    <fragment
        android:name="com.truecaller.settings.impl.ui.messaging.MessagingSettingsFragment"
        android:id="@+id/messaging">
        <argument
            android:name="analytics_context"
            android:defaultValue="settings_screen"
            app:argType="string"/>
        <argument
            android:name="settingItem"
            android:defaultValue="@null"
            app:argType="com.truecaller.settings.impl.ui.messaging.MessagingSettings"
            app:nullable="true"/>
    </fragment>
    <fragment
        android:label="HelpSettingsFragment"
        android:name="com.truecaller.settings.impl.ui.help.HelpSettingsFragment"
        android:id="@+id/help">
        <argument
            android:name="analytics_context"
            android:defaultValue="settings_screen"
            app:argType="string"/>
        <argument
            android:name="settingItem"
            android:defaultValue="@null"
            app:argType="com.truecaller.settings.impl.ui.help.HelpSettings"
            app:nullable="true"/>
    </fragment>
</navigation>
