<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <Transition
        app:constraintSetEnd="@+id/collapsed"
        app:constraintSetStart="@+id/expanded">
        <OnSwipe
            app:dragDirection="dragUp"
            app:touchAnchorId="@+id/content"
            app:touchAnchorSide="top"/>
        <KeyFrameSet>
            <KeyAttribute
                app:framePosition="0"
                app:motionTarget="@+id/actionButtonBar">
                <CustomAttribute
                    app:attributeName="alpha"
                    app:customFloatValue="1"/>
            </KeyAttribute>
            <KeyAttribute
                app:framePosition="30"
                app:motionTarget="@+id/actionButtonBar">
                <CustomAttribute
                    app:attributeName="alpha"
                    app:customFloatValue="0"/>
            </KeyAttribute>
            <KeyAttribute
                app:framePosition="0"
                app:motionTarget="@+id/verifiedNameContainer">
                <CustomAttribute
                    app:attributeName="alpha"
                    app:customFloatValue="1"/>
            </KeyAttribute>
            <KeyAttribute
                app:framePosition="70"
                app:motionTarget="@+id/verifiedNameContainer">
                <CustomAttribute
                    app:attributeName="alpha"
                    app:customFloatValue="0"/>
            </KeyAttribute>
            <KeyAttribute
                app:framePosition="70"
                app:motionTarget="@+id/nameOrNumberShortened">
                <CustomAttribute
                    app:attributeName="alpha"
                    app:customFloatValue="0"/>
            </KeyAttribute>
            <KeyAttribute
                app:framePosition="100"
                app:motionTarget="@+id/nameOrNumberShortened">
                <CustomAttribute
                    app:attributeName="alpha"
                    app:customFloatValue="1"/>
            </KeyAttribute>
            <KeyAttribute
                app:framePosition="0"
                app:motionTarget="@+id/searchWeb">
                <CustomAttribute
                    app:attributeName="alpha"
                    app:customFloatValue="1"/>
            </KeyAttribute>
            <KeyAttribute
                app:framePosition="70"
                app:motionTarget="@+id/searchWeb">
                <CustomAttribute
                    app:attributeName="alpha"
                    app:customFloatValue="0"/>
            </KeyAttribute>
        </KeyFrameSet>
    </Transition>
    <ConstraintSet android:id="@+id/expanded">
        <Constraint
            android:id="@+id/appBar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <Constraint
            android:id="@+id/sourceIcon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:alpha="1"
            android:layout_marginStart="48dp"
            android:layout_marginEnd="4dp"
            app:layout_constraintBottom_toBottomOf="@+id/source"
            app:layout_constraintEnd_toStartOf="@+id/source"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/source"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/source"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="1"
            android:layout_marginEnd="48dp"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toTopOf="@+id/filterMatchReason"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/sourceIcon"
            app:layout_constraintTop_toTopOf="@+id/appBar"
            app:layout_constraintVertical_chainStyle="packed"
            app:layout_goneMarginStart="48dp"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/filterMatchReason"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="1"
            android:layout_marginStart="48dp"
            android:layout_marginEnd="48dp"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toBottomOf="@+id/appBar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/source"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/viewBackdrop"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:alpha="1"
            app:layout_constraintBottom_toTopOf="@+id/avatarMiddleGuideline"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <Constraint
            android:id="@+id/landscapeVideoPlayer"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:alpha="1"
            app:layout_constraintBottom_toBottomOf="@+id/viewBackdrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/vcidExpand"
            android:padding="14dp"
            android:focusable="true"
            android:clickable="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:foreground="?attr/selectableItemBackgroundBorderless"
            android:alpha="1"
            android:elevation="4dp"
            app:layout_constraintBottom_toBottomOf="@+id/viewBackdrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/avatarMiddleGuideline"
            android:layout_width="0dp"
            android:layout_height="1dp"
            app:layout_constraintBottom_toBottomOf="@+id/avatar"
            app:layout_constraintTop_toTopOf="@+id/avatar"/>
        <Constraint
            android:id="@+id/avatar"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:alpha="1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/appBar"/>
        <Constraint
            android:id="@+id/tag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:alpha="1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/avatar"
            app:visibilityMode="ignore"/>
        <Constraint
            android:gravity="center_horizontal"
            android:id="@+id/verifiedNameContainer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingStart="16dp"
            android:paddingEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tag"/>
        <Constraint
            android:id="@+id/nameOrNumberShortened"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleX="0.6825"
            android:scaleY="0.6825"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toBottomOf="@+id/altName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/appBar"/>
        <Constraint
            android:id="@+id/searchWeb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:alpha="1"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constrainedWidth="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/verifiedNameContainer"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/privateUserInformation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:alpha="1"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constrainedWidth="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/searchWeb"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/altName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleX="1"
            android:scaleY="1"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constrainedWidth="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/searchWeb"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/nameBarrier"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:barrierDirection="bottom"
            app:constraint_referenced_ids="altName"/>
        <Constraint
            android:id="@+id/businessAwarenessCard"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/doubleSpace"
            android:layout_marginEnd="@dimen/doubleSpace"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/nameBarrier"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/presenceWithTimezone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:scaleX="1"
            android:scaleY="1"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/businessAwarenessCard"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/presenceTimezoneBarrier"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:barrierDirection="bottom"
            app:constraint_referenced_ids="presenceWithTimezone"/>
        <Constraint
            android:id="@+id/spamCategoryIcon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:alpha="1"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="4dp"
            app:layout_constraintBottom_toBottomOf="@+id/spamCategoryAndCount"
            app:layout_constraintEnd_toStartOf="@+id/spamCategoryAndCount"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/spamCategoryAndCount"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/spamCategoryAndCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:alpha="1"
            android:layout_marginEnd="16dp"
            app:layout_constrainedWidth="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toEndOf="@+id/spamCategoryIcon"
            app:layout_constraintTop_toBottomOf="@+id/presenceTimezoneBarrier"
            app:layout_goneMarginStart="16dp"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/trueContext"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:alpha="1"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintBottom_toTopOf="@+id/surveyContainer"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/spamCategoryAndCount"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/actionButtonBar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/surveyContainer"/>
        <Constraint
            android:id="@+id/headerDivider"
            android:layout_width="0dp"
            android:layout_height="4dp"
            android:alpha="0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/actionButtonBar"/>
        <Constraint
            android:id="@+id/headerBackground"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/headerDivider"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <CustomAttribute
                app:attributeName="backgroundColor"
                app:customColorValue="?attr/tcx_backgroundPrimary"/>
        </Constraint>
        <Constraint
            android:id="@+id/content"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/actionButtonBar"/>
        <Constraint
            android:id="@+id/fab"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/surveyContainer"
            android:visibility="visible"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintBottom_toTopOf="@+id/actionButtonBar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/trueContext"/>
    </ConstraintSet>
    <ConstraintSet android:id="@+id/collapsed">
        <Constraint
            android:id="@+id/appBar"
            android:layout_width="0dp"
            android:layout_height="?attr/actionBarSize"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <Constraint
            android:id="@+id/sourceIcon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:alpha="0"
            android:layout_marginEnd="4dp"
            app:layout_constraintBottom_toBottomOf="@+id/source"
            app:layout_constraintEnd_toStartOf="@+id/source"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/source"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/source"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0"
            app:layout_constraintBottom_toTopOf="@+id/filterMatchReason"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/sourceIcon"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/filterMatchReason"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0"
            app:layout_constraintBottom_toTopOf="@+id/avatar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/viewBackdrop"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:alpha="0"
            app:layout_constrainedHeight="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <Constraint
            android:id="@+id/landscapeVideoPlayer"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:alpha="0"
            app:layout_constraintBottom_toBottomOf="@+id/viewBackdrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/vcidExpand"
            android:padding="14dp"
            android:focusable="true"
            android:clickable="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:foreground="?attr/selectableItemBackgroundBorderless"
            android:alpha="0"
            android:elevation="4dp"
            app:layout_constraintBottom_toBottomOf="@+id/viewBackdrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/avatarMiddleGuideline"
            android:layout_width="0dp"
            android:layout_height="1dp"
            app:layout_constraintBottom_toBottomOf="@+id/avatar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/avatar"/>
        <Constraint
            android:id="@+id/avatar"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:alpha="0"
            app:layout_constraintBottom_toTopOf="@+id/tag"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
        <Constraint
            android:id="@+id/tag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:alpha="0"
            app:layout_constraintBottom_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
        <Constraint
            android:gravity="center_horizontal"
            android:id="@+id/verifiedNameContainer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingStart="16dp"
            android:paddingEnd="16dp"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toBottomOf="@+id/appBar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/nameOrNumberShortened"/>
        <Constraint
            android:id="@+id/nameOrNumberShortened"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleX="0.6825"
            android:scaleY="0.6825"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toBottomOf="@+id/altName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/appBar"/>
        <Constraint
            android:id="@+id/searchWeb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:alpha="0"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toBottomOf="@+id/appBar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/verifiedNameContainer"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/privateUserInformation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:alpha="0"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toBottomOf="@+id/appBar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/verifiedNameContainer"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/altName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:scaleX="0.75"
            android:scaleY="0.75"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constrainedWidth="true"
            app:layout_constraintBottom_toBottomOf="@+id/appBar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/nameOrNumberShortened"/>
        <Constraint
            android:id="@+id/presenceWithTimezone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:alpha="0"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/altName"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/spamCategoryIcon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:alpha="0"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="4dp"
            app:layout_constraintBottom_toBottomOf="@+id/spamCategoryAndCount"
            app:layout_constraintEnd_toStartOf="@+id/spamCategoryAndCount"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/spamCategoryAndCount"/>
        <Constraint
            android:id="@+id/spamCategoryAndCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:alpha="0"
            app:layout_constrainedWidth="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/spamCategoryIcon"
            app:layout_constraintTop_toTopOf="@+id/altName"/>
        <Constraint
            android:id="@+id/businessAwarenessCard"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:alpha="0"
            android:layout_marginStart="@dimen/doubleSpace"
            android:layout_marginEnd="@dimen/doubleSpace"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/altName"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/trueContext"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:alpha="0"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/altName"/>
        <Constraint
            android:id="@+id/actionButtonBar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/appBar"/>
        <Constraint
            android:id="@+id/headerDivider"
            android:layout_width="0dp"
            android:layout_height="4dp"
            android:alpha="1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/actionButtonBar"/>
        <Constraint
            android:id="@+id/headerBackground"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/headerDivider"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <CustomAttribute
                app:attributeName="backgroundColor"
                app:customColorValue="?attr/tcx_detailsViewHeaderCollapsedBackgroundColor"/>
        </Constraint>
        <Constraint
            android:id="@+id/content"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginTop="128dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/actionButtonBar"/>
        <Constraint
            android:id="@+id/fab"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:visibilityMode="ignore"/>
        <Constraint
            android:id="@+id/surveyContainer"
            android:visibility="invisible"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/appBar"/>
    </ConstraintSet>
</MotionScene>
